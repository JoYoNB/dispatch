<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace命名规则: 表对应java类的名称-->
<mapper namespace="com.chainway.dispatcherservice.biz.dao.CarryTypeDao">
	<select id="getCarryTypeList" resultType="java.util.Map">
		SELECT carry_type_id as id ,carry_type_name as name FROM t_vehicle_carry_type_dict;
	</select>
</mapper>